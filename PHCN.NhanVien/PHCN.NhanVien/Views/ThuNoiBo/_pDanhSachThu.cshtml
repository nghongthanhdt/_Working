
@{ 
     var listGuiNhan = ViewBag.ListGuiNhan;

}

<table class="table table-condensed">
    <thead>
        <tr>
            <th style="width:3%">#</th>
            <th style="width:57%">Tiêu đề</th>
            <th style="width:20%">Người gửi</th>
            <th style="width:20%">Ngày</th>
        </tr>
    </thead>
    <tbody>
        @foreach (PHCN.NhanVien.Models.GuiNhan item in listGuiNhan)
        {
            <tr>
                <td>1</td>
                <td>
                    
                    <a href="javascript:void(0);">@item.BaiViet.TieuDe</a>
                </td>
                <td>@item.NhanVien.HoTen</td> @*(người gửi)*@
                <td>@item.BaiViet.Ngay</td>
            </tr>
        }
        
    </tbody>
</table>
